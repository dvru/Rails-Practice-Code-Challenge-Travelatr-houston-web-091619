
<h1> Name: <%= @blogger.name %></h1>
<h1> Age: <%= @blogger.age %></h1>
<p> Bio: <%= @blogger.bio %></p>
<p> Total # of likes: <%= @likes %> </p>

<%= button_to "All Bloggers", bloggers_path, method: :get %>
<%= button_to "Edit Blooger", edit_blogger_path(@blogger), method: :get %>
<%= button_to "Remove Blogger", blogger_path(@blogger), method: 'delete', data: { confirm: 'Are you sure' } %>
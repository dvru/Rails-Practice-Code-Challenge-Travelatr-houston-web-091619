<% if flash[:errors] %>
<ul style=“color: red;”>
	<% flash[:errors].each do |error| %>
		<li> <%= error %> </li>
	<% end %>
</ul>
<% end %>



<%= form_for @blogger do |f| %>
<%= f.label :name %>
<%= f.text_field :name %>
<%= f.label :bio %>
<%= f.text_field :bio %>
<%= f.label :age %>
<%= f.text_field :age %>
<%= f.submit %>
<% end %>